#!/bin/sh

# Wallpaper:
eval `cat $HOME/.fehbg`

# Composite management:
xcompmgr &

# Keymaps:
xmodmap .Xmodmap

# Keyboard repeat rate:
xset r rate 300 20

# Applications:
# autokey-gtk &
(sleep 5 && transmission-gtk --minimized) &
(sleep 10 && skype) &
(sleep 15 && qgmailnotifier) &

exec awesome
# exec ck-launch-session gnome-session
# exec startkde
# exec startxfce4
# exec icewm
# exec openbox
# exec blackbox
# exec fluxbox
# exec ratpoison
# exec dwm
